read: term -> {
    ~
    ch -> {
        {
            term read ()
            ch
        }

        {}

        ch term = ()

        ? () ()
    }
    ()
}

print: c -> {
    {
        c ,
        print ()
    }

    {}

    c 0 = ()

    ? () ()
}

0 "What is your name? " print ()
0 10 read () 
"Hello, " print ()
